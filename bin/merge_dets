#!/bin/env python

"""This script aims to merge multiple list of TODs into one.
Note that it only acts on det_uid field

Usage:
>>> ./merge_dets *.dict > output.dict

"""

import sys
from moby2.util import MobyDict

dict_list = sys.argv[1:]
all_dets = set()
for l in dict_list:
    all_dets = all_dets.union(set(MobyDict.from_file(l)['det_uid']))
print("det_uid = %s" % list(all_dets))
